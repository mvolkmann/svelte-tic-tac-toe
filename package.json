    "@testing-library/svelte": "^3.0.3",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.4",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
